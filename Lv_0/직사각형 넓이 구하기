# https://school.programmers.co.kr/learn/courses/30/lessons/120860

def solution(dots):
    answer = 0
    if(dots[0][0] != dots[1][0]):
        x = abs(dots[0][0] - dots[1][0])
    else:
        x = abs(dots[0][0] - dots[2][0])
    if(dots[0][1] != dots[1][1]):
        y = abs(dots[0][1] - dots[1][1])
    else:
        y = abs(dots[0][1] - dots[2][1])
        
    answer = x*y
    
    return answer
