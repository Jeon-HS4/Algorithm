# https://school.programmers.co.kr/learn/courses/30/lessons/120843

def solution(numbers, k):
    answer = 0
    n=0
    while(k>0):
        answer = numbers[n]
        if(n+2 > len(numbers)-1):
            n = n +2 -len(numbers)
        else:
            n += 2
        k -= 1
        
    return answer
